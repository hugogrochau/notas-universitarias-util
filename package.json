  "description": "Biblioteca para calcular média e aprovação de disciplinas universitárias",
  "main": "dist/index.js",
  "scripts": {
    "build": "babel src -D -d dist --ignore node_modules",
    "lint": "eslint src test",
    "lint:staged": "lint-staged",
